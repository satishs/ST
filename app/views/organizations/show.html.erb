<div id='display'>
    <p>
      <b>Name:</b>
      <%=h @organization.name %>
    </p>

    <p>
      <b>Url:</b>
      <%=h @organization.url %>
    </p>

    <p>
      <b>Description:</b>
      <%=h @organization.description %>
    </p>

<% if current_user %>
    <%= link_to 'Edit', edit_organization_path(@organization) %> |
<% end %>
<%= link_to 'Back', organizations_path %>

</div>